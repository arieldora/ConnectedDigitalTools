*only retain trials with keywords 
*	i.e. whether they had a keyword in brief summary or design groups or design outcomes, etc
keep if merge_outmeas==3|merge_outdesc==3|merge_detdesc==3|merge_brief_summ==3|merge_intv==3|merge_dgroups==3|merge_offtitle==3
rename merge* source*
compress
save temp/studies_keep.dta, replace


*Grab additional fields of interest
***********************************
*for the purpose of our analyses, for those trials with keywords
*we also want to know their associated conditions, location of trial(countries), and sponsor info

***Conditions
u temp/conditions, clear
*only grab conditions for trials with keywords
merge m:1 nct_id using temp/studies_keep.dta,keep(3) keepusing(nct_id)
keep nct_id name
*get the dataset at trial level
*	this requires reshaping the dataset wide
duplicates drop
gen dum=1
sort nct_id
bysort nct_id: gen counter=sum(dum)
drop dum
rename name conditions
reshape wide conditions, i(nct_id) j(counter)
egen name_ = concat(conditions*), punct("|")
replace name_=subinstr(name_,"||","",.)
replace name_=trim(name_)
replace name_= substr(name_,1,length(name_)-1) if substr(name_,-1,1)=="|"
keep nct_id name_
rename name_ conditions
duplicates drop
compress
save temp/conditions_keep.dta, replace


****countries
u temp/countries, clear
*only grab countries for trials with keywords
merge m:1 nct_id using temp/studies_keep.dta,keep(3)
keep nct_id name
*get the dataset at trial level
*	this requires reshaping the dataset wide
duplicates drop
gen dum=1
sort nct_id
bysort nct_id: gen counter=sum(dum)
drop dum
reshape wide name, i(nct_id) j(counter)
egen countries = concat(name*), punct("|")
replace countries=subinstr(countries,"||","",.)
replace countries=trim(countries)
replace countries= substr(countries,1,length(countries)-1) if substr(countries,-1,1)=="|"
keep nct_id countries
duplicates drop
compress
save temp/countries_keep.dta, replace


****Sponsors
u temp/sponsors, clear
*only grab sponsors for trials with keywords
merge m:1 nct_id using temp/studies_keep.dta,keep(3)
drop _merge
gen lead=name if lead_or_collaborator=="lead"
gen collab=name if lead_or_collaborator=="collaborator"
keep nct_id lead collab
*get the dataset at trial level
*	this requires reshaping the dataset wide
duplicates drop
gen dum=1
sort nct_id
bysort nct_id: gen counter=sum(dum)
drop dum
reshape wide lead collab, i(nct_id) j(counter)
egen sponsors = concat(lead*), punct("|")
replace sponsors=subinstr(sponsors,"||","",.)
replace sponsors=trim(sponsors)
replace sponsors= substr(sponsors,1,length(sponsors)-1) if substr(sponsors,-1,1)=="|"
egen collaborators = concat(collab*), punct("|")
replace collaborators=subinstr(collaborators,"||","",.)
replace collaborators=trim(collaborators)
replace collaborators= substr(collaborators,1,length(collaborators)-1) if substr(collaborators,-1,1)=="|"
replace sponsors = subinstr(sponsors, "|", "", 1) 
replace collaborators = subinstr(collaborators, "|", "", 1) 
keep nct_id sponsors collaborators
npresent
compress
save temp/sponsors_keep.dta, replace


*Create industry dummies
u temp/sponsors, clear
*only create industry dummies for trials with keywords
merge m:1 nct_id using temp/studies_keep.dta,keep(3) keepusing(nct_id)
gen industry=(agency_class=="Industry")
gen non_industry=(industry==0)
*get the dataset at trial level
